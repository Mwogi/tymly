{
  "boardTitleTemplate": "Incident ${incident.incidentNumber}/${incident.incidentYear}",
  "watchable": true,
  "category": "incidents",
  "content": [
    {
      "widget": "summary",
      "config": {
        "template": "We can show some HTML stuff here"
      }
    },
    {
      "widget": "maplist",
      "config": {
        "header": "Map",
        "map": {
          "latProperty": "lat",
          "lonProperty": "lon",
          "editable": false,
          "drawing": false,
          "layers": ["hydrants"],
          "3d": false,
          "initialZoom": "4"
        },
        "properties": [
          {
            "prompt": "Over border",
            "dataPath": "incident.overBorder",
            "showIfUndefined": true
          },
          {
            "prompt": "Station area",
            "dataPath": "incident.stationArea",
            "showIfUndefined": true
          },
          {
            "prompt": "Station responsible",
            "dataPath": "incident.stationResponsible",
            "showIfUndefined": true
          },
          {
            "prompt": "Station call",
            "dataPath": "incident.stationCall",
            "showIfUndefined": true
          },
          {
            "prompt": "Position latitude",
            "dataPath": "incident.lat",
            "showIfUndefined": true
          },
          {
            "prompt": "Position longitude",
            "dataPath": "incident.long",
            "showIfUndefined": true
          },
          {
            "prompt": "Address",
            "dataPath": "incident.addressLines",
            "showIfUndefined": true
          }
        ]
      }
    },
    {
      "widget": "propertylist",
      "config": {
        "header": "Summary",
        "properties": [
          {
            "prompt": "Resource usage",
            "dataPath": "incident.resourceSummary.byCallsignType",
            "showIfUndefined": true
          },
          {
            "prompt": "Attendence summary",
            "dataPath": "incident.resourceSummary.byAttendance",
            "showIfUndefined": true
          },
          {
            "prompt": "First Mobilised",
            "dataPath": "incident.resourceSummary.firstMobilised",
            "showIfUndefined": true
          },
          {
            "prompt": "First in Attendance",
            "dataPath": "incident.resourceSummary.firstInAttendance",
            "showIfUndefined": true
          },
          {
            "prompt": "First Stop",
            "dataPath": "incident.resourceSummary.firstStop",
            "showIfUndefined": true
          },
          {
            "prompt": "Last Appliance Returned",
            "dataPath": "incident.resourceSummary.lastApplianceReturned",
            "showIfUndefined": true
          },
          {
            "prompt": "Reaction Time",
            "dataPath": "incident.resourceSummary.reactionTime",
            "showIfUndefined": true
          },
          {
            "prompt": "Response Time",
            "dataPath": "incident.resourceSummary.responseTime",
            "showIfUndefined": true
          }
        ]
      }
    }
  ]
}