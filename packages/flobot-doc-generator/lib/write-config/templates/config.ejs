baseurl = "https://wmfs.github.io/"
languageCode = "en-us"
title = "Tymly Documentation"
theme = "hugo-material-docs"
metadataformat = "yaml"
canonifyurls = true
# Enable Google Analytics by entering your tracking id
googleAnalytics = "UA-90924394-1"

[params]
	# General information
	author = "Tim Needham"
	description = "An open source low-code platform - that's built for collaboration"
	copyright = "MIT"

	# Repository
	provider = "GitHub"
	repo_url = "https://github.com/wmfs/tymly"

	version = "<%= flobotPackage.version %>"
	logo = "images/logo.png"
	favicon = "images/favicon.png"

	permalink = "#"

	# Custom assets
	custom_css = ["tymly.css"]
	custom_js  = []

	# Syntax highlighting theme
	highlight_css  = ""

	[params.palette]
    	primary = "teal"
    	accent  = "blue-grey"

    [params.font]
    	text = "Ubuntu"
    	code = "Ubuntu Mono"


[social]
	twitter = "flobotJS"
	github  = "wmfs/tymly"


[[menu.main]]
	name   = "Introduction"
	url    = "/"
	weight = 0

[[menu.main]]
	name   = "Key Concepts"
	url    = "key-concepts/"
	weight = 10

[[menu.main]]
	name   = "Getting started"
	url    = "getting-started/"
	weight = 20

[[menu.main]]
	name   = "Reference"
	url    = "reference/"
	weight = 30

[[menu.main]]
    name   = "License"
    url    = "license/"
    weight = 40


[blackfriday]
	smartypants	= true
	fractions = true
	smartDashes = true
	plainIDAnchors = true
